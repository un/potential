{
  "name": "@potential/mobile",
  "version": "0.1.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "clean": "git clean -xdf .cache .expo .turbo android ios node_modules",
    "dev:mobile": "pnpm with-env expo start",
    "dev:android": "pnpm with-env expo start --android",
    "dev:ios": "pnpm with-env expo start --ios",
    "build:ios": "NODE_ENV=production eas build --platform ios --profile production  --message \"First Proper Build?\" --local",
    "android": "pnpm with-env expo run:android",
    "ios": "pnpm with-env expo run:ios",
    "device": "pnpm with-env expo run:ios --device",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path .prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "prebuild": "cd ../.. && pnpm install && pnpm turbo build && cd apps/mobile"
  },
  "dependencies": {
    "@better-auth/expo": "catalog:",
    "@expo-google-fonts/ibm-plex-sans": "0.3.0",
    "@expo-google-fonts/ibm-plex-serif": "0.3.0",
    "@expo/metro-config": "^0.20.12",
    "@potential/auth": "workspace:*",
    "@potential/consts": "workspace:*",
    "@potential/storage": "workspace:*",
    "@potential/templates": "workspace:*",
    "@potential/utils": "workspace:*",
    "@react-native-picker/picker": "^2.11.0",
    "@rn-primitives/hooks": "^1.2.0",
    "@rn-primitives/portal": "^1.2.0",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "@rn-primitives/utils": "^1.1.0",
    "@shopify/flash-list": "1.7.6",
    "@tanstack/react-form": "catalog:",
    "@tanstack/react-query": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "@trpc/tanstack-react-query": "catalog:",
    "better-auth": "catalog:",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "53.0.9",
    "expo-av": "15.1.4",
    "expo-camera": "16.1.6",
    "expo-constants": "17.1.6",
    "expo-dev-client": "5.1.8",
    "expo-file-system": "18.1.10",
    "expo-font": "13.3.1",
    "expo-image": "2.1.7",
    "expo-image-manipulator": "13.1.7",
    "expo-image-picker": "16.1.4",
    "expo-linking": "7.1.5",
    "expo-localization": "^16.1.5",
    "expo-router": "5.0.7",
    "expo-secure-store": "14.2.3",
    "expo-splash-screen": "0.30.8",
    "expo-status-bar": "2.2.3",
    "expo-updates": "~0.28.13",
    "expo-web-browser": "14.1.6",
    "input-otp-native": "^0.4.1",
    "nativewind": "~4.1.23",
    "phosphor-react-native": "^2.3.1",
    "radi-color-css": "^1.0.5",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-native": "0.79.2",
    "react-native-css-interop": "~0.1.22",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "~5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "^15.11.2",
    "sonner-native": "^0.19.0",
    "superjson": "2.2.2",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/runtime": "7.27.1",
    "@potential/eslint-config": "workspace:*",
    "@potential/prettier-config": "workspace:*",
    "@potential/tailwind-config": "workspace:*",
    "@potential/trpc": "workspace:*",
    "@potential/tsconfig": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/react": "~19.0.10",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@potential/prettier-config",
  "expo": {
    "install": {
      "exclude": [
        "@types/react"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "@potential/auth",
          "@potential/consts",
          "@potential/storage",
          "@potential/templates",
          "@potential/utils",
          "@tanstack/react-form",
          "@trpc/client",
          "@trpc/server",
          "@trpc/tanstack-react-query",
          "react-native-element-dropdown"
        ],
        "listUnknownPackages": false
      }
    }
  }
}