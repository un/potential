{
  "name": "@potential/auth",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --outfile=dist/index.js --platform=node --format=esm --bundle --external:@better-auth/expo --external:@potential/consts --external:@potential/db --external:@potential/email --external:@potential/env --external:@potential/utils --external:@potential/validators --external:better-auth --external:dotenv --external:zod && tsc -p tsconfig.json --emitDeclarationOnly --declaration --declarationDir dist",
    "check": "tsc --noEmit",
    "auth:generate": "drizzle-kit push"
  },
  "dependencies": {
    "@better-auth/expo": "catalog:",
    "@potential/consts": "workspace:*",
    "@potential/db": "workspace:*",
    "@potential/email": "workspace:*",
    "@potential/env": "workspace:*",
    "@potential/utils": "workspace:*",
    "@potential/validators": "workspace:*",
    "better-auth": "catalog:",
    "dotenv": "^16.5.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@potential/prettier-config": "workspace:*",
    "@potential/tsconfig": "workspace:*",
    "esbuild": "^0.25.4",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "^4.19.4",
    "typescript": "catalog:"
  },
  "prettier": "@potential/prettier-config"
}