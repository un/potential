{
    "name": "@potential/consts",
    "private": true,
    "version": "0.3.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "esbuild src/index.ts --outfile=dist/index.js --platform=node --format=esm --bundle && tsc -p tsconfig.json --emitDeclarationOnly --declaration --declarationDir dist",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@potential/prettier-config": "workspace:*",
        "@potential/tsconfig": "workspace:*",
        "@potential/utils": "workspace:*",
        "esbuild": "^0.25.4",
        "eslint": "catalog:",
        "prettier": "catalog:",
        "typescript": "catalog:",
        "zod": "catalog:"
    },
    "prettier": "@potential/prettier-config"
}